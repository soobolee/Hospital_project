package kr.or.ddit.medical.vo;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.RequiredArgsConstructor;

/*
 * lombok 어노테이션
 * @RequiredArgsConstructor : 추가 작업을 필요로 하는 필드에 대한 생성자를 생성한다.
 * @AllArgsConstructor : 클래스에 존재하는 모든 필드에 대한 생성자를 자동으로 생성해 준다.
 * @Data : @Getter, @Setter, @RequiredArgsConstructor, @ToString, @EqualsAndHashCode를 한 번에 설정한다.
 */
@RequiredArgsConstructor
@AllArgsConstructor
@Data
public class MedicalVo {
	private String pntCd; // 환자코드
	private String pntNm; // 환자이름
	private String telno; // 환자전화번호
	private String pntPrno; // 환자주민번호
	private String addr; // 주소
	private String stateCd; // 환자상태
	private String age; // 나이
	private String symptoms; // 증상
	private String cmntNo; // 코멘트 번호
	private String cmntDt; // 코멘트 작성일
	private String cmntCont; // 코멘트 내용
	private String rsvtNo; // 예약번호
}
